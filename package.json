{
  "name": "medical-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed-pacientes": "ts-node ./src/entities/seed-pacientes.ts",
    "seed-consultas": "ts-node ./src/entities/seed-consultas.ts",
    "seed-antecedentes": "ts-node ./src/entities/seed-antecedentes-personales.ts",
    "seed-examen-general": "ts-node ./src/entities/seed-examenes-generales.ts",
    "seed-examen-regional": "ts-node ./src/entities/seed-examen-fisico-regional.ts",
    "seed-examen-complementario": "ts-node ./src/entities/seed-examenes-complementarios.ts",
    "seed-diagnostico": "ts-node ./src/entities/seed-diagnosticos-tratamientos.ts",
    "migrate": "npx typeorm migration:run",
    "migration:create": "npx typeorm migration:create",
    "migration:generate": "npx typeorm migration:generate",
    "migration:revert": "npx typeorm migration:revert"
  },
  "dependencies": {
    "@nestjs/common": "^10.2.10",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.10",
    "@nestjs/platform-express": "^10.2.10",
    "@nestjs/platform-socket.io": "^10.2.10",
    "@nestjs/typeorm": "^10.0.1",
    "@nestjs/websockets": "^10.2.10",
    "@types/luxon": "^3.4.0",
    "@types/socket.io": "^3.0.2",
    "luxon": "^3.4.4",
    "mysql2": "^3.6.5",
    "pdfkit": "^0.14.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "socket.io": "^4.7.2",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.2.10",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.11",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.3.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
